<?xml version="1.0" encoding="UTF-8"?>
<element>
	<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
		version="3.1">
		
		<display-name>LoginUsingFilters</display-name>
		<servlet>
			<servlet-name>abc</servlet-name>
			<servlet-class>com.chandu.SerializeData</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>abc</servlet-name>
			<url-pattern>/SerializeData</url-pattern>
		</servlet-mapping>
		<servlet>
			<servlet-name>abcd</servlet-name>
			<servlet-class>/loginuser</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>abcd</servlet-name>
			<url-pattern>/loginuser</url-pattern>
		</servlet-mapping>
		

		<welcome-file-list>
			<welcome-file>login.jsp</welcome-file>
		</welcome-file-list>
		
	</web-app>
	</element>

